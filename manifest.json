{
    "manifest_version": 2,
    "name": "YT Mobile Loudness averaging",
    "version": "1.0.0",
    "description": "",
    "applications": {
        "gecko": {
            "id": "yt-mobile-loudness-averaging@typeling1578",
            "strict_min_version": "102.0"
        }
    },
    "content_scripts": [
        {
            "run_at": "document_idle",
            "matches": [
                "*://m.youtube.com/*"
            ],
            "js": [
                "content_script.js"
            ]
        }
    ]
}